@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div id="throbber" style="display:none; min-height:120px;"></div>
    <div id="noty-holder"></div>
    <div id="wrapper">
        <!-- Navigation -->
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="http://cijulenlinea.ucr.ac.cr/dev-users/">
                    <img src="~/images/logo2.png" alt="logo" width="200" height="50">
                </a>
            </div>
            <!-- Top Menu Items -->
            <ul class="nav navbar-right top-nav">
                @if (!String.IsNullOrEmpty((string)HttpContext.Current.Session["AdminId"]))
                {
                    using (Html.BeginForm("Logout", "Logins", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
                    {
                        <ul class="nav navbar-nav navbar-right">
                            <li>
                                <a href="#" >Welcome Admin</a>
                            </li>
                            <li><a href="javascript:document.getElementById('logoutForm').submit()">Logout</a></li>
                        </ul>
                    }
                }
                else
                {
                    <li><a href="@Url.Action("Login", "Logins")"><button type="button" class="btn btn-danger">Login</button></a></li>
                }
            </ul>
            <!-- Sidebar Menu Items - These collapse to the responsive navigation menu on small screens -->
            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul class="nav navbar-nav side-nav">
                    <li>
                        <a href="#" data-toggle="collapse" data-target="#submenu-1"><i class="fa fa-fw fa-star"></i>ORPHAN<i class="fa fa-fw fa-angle-down pull-right"></i></a>
                        <ul id="submenu-1" class="collapse">
                            <li><a href="@Url.Action("Index","Orphans","")"><i class="fa fa-angle-double-right"></i>LIST</a></li>
                            <li><a href="@Url.Action("Create","Orphans","")"><i class="fa fa-angle-double-right"></i> CREATE</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="#" data-toggle="collapse" data-target="#submenu-2"><i class="fa fa-fw fa-star"></i>ORPHAN BACKGROUND<i class="fa fa-fw fa-angle-down pull-right"></i></a>
                        <ul id="submenu-2" class="collapse">
                            <li><a href="@Url.Action("Index","OrphanBackgrounds","")"><i class="fa fa-angle-double-right"></i>LIST</a></li>
                            <li><a href="@Url.Action("Create","OrphanBackgrounds","")"><i class="fa fa-angle-double-right"></i> CREATE</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="#" data-toggle="collapse" data-target="#submenu-3" ><i class="fa fa-fw fa-star"></i>SUPERVISORS<i class="fa fa-fw fa-angle-down pull-right"></i></a>
                        <ul id="submenu-3" class="collapse">
                            <li><a href="@Url.Action("Index","Supervisors","")"><i class="fa fa-angle-double-right"></i>LIST</a></li>
                            <li><a href="@Url.Action("Create","Supervisors","")"><i class="fa fa-angle-double-right"></i> CREATE</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="#" data-toggle="collapse" data-target="#submenu-4"><i class="fa fa-fw fa-star"></i>ADOPTION REQUEST<i class="fa fa-fw fa-angle-down pull-right"></i></a>
                        <ul id="submenu-4" class="collapse">
                            <li><a href="@Url.Action("Index","Adoptions","")"><i class="fa fa-angle-double-right"></i>LIST</a></li>
                        </ul>
                    </li>
                    <li>
                        <a href="#" data-toggle="collapse" data-target="#submenu-5"><i class="fa fa-fw fa-star"></i>SPONSORSHIP<i class="fa fa-fw fa-angle-down pull-right"></i></a>
                        <ul id="submenu-5" class="collapse">
                            <li><a href="@Url.Action("Index","Sponsors","")"><i class="fa fa-angle-double-right"></i>LIST</a></li>
                        </ul>
                    </li>
                   
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </nav>
        <div id="page-wrapper">
            <div class="container-fluid">
                <!-- Page Heading -->
                <div class="row" id="main">
                </div><!-- /#wrapper -->

</body>
</html>
